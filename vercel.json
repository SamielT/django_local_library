{
  "version": 2,
  "builds": [
    {
      "src": "locallibrary/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "locallibrary/wsgi.py"
    }
  ],
  "buildCommand": "python3 manage.py migrate"
}
